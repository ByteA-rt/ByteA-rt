def feast(beast, dish):
    # your code here
    
    #Code for best
    length = len(beast) - 1
    first = beast[0]
    last = beast[(length)]

    #Code for dish
    length_dish = len(dish) - 1
    first_dish = dish[0]
    last_dish = dish[(length_dish)]

    if first == first_dish and last == last_dish:
        return True
    else:
        return False
        pass
