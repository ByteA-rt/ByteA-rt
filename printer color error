def printer_error(s):
 
   length_string =len(s)
    s_array = []
    check = ("abcdefghijklm")
    check_array = []
    error_count = 0
    
    for i in range(len(check)):
        check_array.append(check[i])
    print (check_array)
    
    for i in range(len(s)):
        s_array.append(s[i])
    print (s_array)
    
    for i in range(len(s_array)):
        if s_array[i] in check_array:
            pass
        else: error_count += 1
    print (error_count)
    
 return (f'{error_count}/{length_string}')
